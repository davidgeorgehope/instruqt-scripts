{
  "params": {
    "timeSize": 5,
    "timeUnit": "m",
    "logView": {
      "type": "log-view-reference",
      "logViewId": "default"
    },
    "count": {
      "value": 75,
      "comparator": "more than"
    },
    "criteria": [
      {
        "field": "log.file.path",
        "comparator": "equals",
        "value": "/var/log/mysql/error.log"
      }
    ]
  },
  "consumer": "alerts",
  "schedule": {
    "interval": "1m"
  },
  "tags": [],
  "name": "Log Spike",
  "rule_type_id": "logs.alert.document.count",
  "actions": []
}